local function updateStatus(text, color)
    StatusLabel.Text = text
    StatusLabel.TextColor3 = color
end

local function findPlayerPartial(text)
    for _, p in pairs(Players:GetPlayers()) do
        if string.find(string.lower(p.Name), string.lower(text)) or string.find(string.lower(p.DisplayName), string.lower(text)) then
            return p
        end
    end
    return nil
end

local function getNearestPlayerTo(targetP)
    local closest = nil
    local dist = 999999
    if targetP and targetP.Character and targetP.Character:FindFirstChild("HumanoidRootPart") then
        local tPos = targetP.Character.HumanoidRootPart.Position
        for _, p in pairs(Players:GetPlayers()) do
            if p ~= LocalPlayer and p ~= targetP and p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
                local d = (p.Character.HumanoidRootPart.Position - tPos).Magnitude
                if d < dist then
                    dist = d
                    closest = p
                end
            end
        end
    end
    return closest
end

local function triggerJumpscare()
    local sc = Instance.new("ScreenGui")
    sc.Name = "JumpscareGUI"
    sc.Parent = LocalPlayer.PlayerGui
    sc.IgnoreGuiInset = true
    local img = Instance.new("ImageLabel")
    img.Parent = sc
    img.Size = UDim2.new(1,0,1,0)
    img.Image = "rbxassetid://5565362953"
    img.BackgroundColor3 = Color3.new(0,0,0)
    local snd = Instance.new("Sound")
    snd.Parent = sc
    snd.SoundId = "rbxassetid://9120339906"
    snd.Volume = 10
    snd.PlayOnRemove = true
    snd:Play()
    task.wait(2)
    sc:Destroy()
end

local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
    Name = "Venus Controlled",
    LoadingTitle = "Venus Control Logic",
    LoadingSubtitle = "weird controls",
    Theme = "DarkBlue",
    ConfigurationSaving = {
        Enabled = true,
        FileName = "VenusControlConfig"
    },
})

local function Notify(title, content, duration)
    Rayfield:Notify({
        Title = title,
        Content = content,
        Duration = duration or 3,
        Image = 4483362458,
    })
end

local function standLogic()
    if isControlled and controllerPlayer and controllerPlayer.Character and controllerPlayer.Character:FindFirstChild("HumanoidRootPart") then
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
            if not isBusy and not orbitEnabled and not hiddenfling then
                local char = LocalPlayer.Character
                local tRoot = controllerPlayer.Character.HumanoidRootPart
                local cRoot = char.HumanoidRootPart
                local hum = char:FindFirstChild("Humanoid")
                if hum then
                    hum:ChangeState(Enum.HumanoidStateType.Physics)
                end
                
                if not standPlatform then
                    standPlatform = Instance.new("Part")
                    standPlatform.Size = Vector3.new(4, 0.5, 4)
                    standPlatform.Transparency = 1
                    standPlatform.Anchored = true
                    standPlatform.CanCollide = true
                    standPlatform.Name = "StandPlatform"
                    standPlatform.Parent = workspace
                end
                
                local bob = math.sin(tick() * 2) * 0.8
                
                local goalCF = tRoot.CFrame * CFrame.new(0, 3 + bob, 2.5)
                
                if dogMode then
                    goalCF = tRoot.CFrame * CFrame.new(2, -2, 2)
                end

                if standPlatform then
                    standPlatform.CFrame = goalCF * CFrame.new(0, -3, 0)
                end
                cRoot.CFrame = cRoot.CFrame:Lerp(goalCF, 0.15)
                cRoot.Velocity = Vector3.zero
            elseif orbitEnabled and not hiddenfling then
                local char = LocalPlayer.Character
                local tRoot = controllerPlayer.Character.HumanoidRootPart
                local cRoot = char.HumanoidRootPart
                local hum = char:FindFirstChild("Humanoid")
                if hum then
                   hum:ChangeState(Enum.HumanoidStateType.Physics) 
                end
                local angle = tick() * 3
                local radius = 8
                local offset = Vector3.new(math.cos(angle) * radius, 0, math.sin(angle) * radius)
                local goalCF = tRoot.CFrame + offset
                cRoot.CFrame = CFrame.new(goalCF.Position, tRoot.Position)
                cRoot.Velocity = Vector3.zero
            else
                if standPlatform then standPlatform.CFrame = CFrame.new(0, -500, 0) end
            end
        end
    else
        if standPlatform then standPlatform:Destroy() standPlatform = nil end
    end
end
RunService.Heartbeat:Connect(standLogic)

RunService.Heartbeat:Connect(function()
    if spinEnabled and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        LocalPlayer.Character.HumanoidRootPart.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.Angles(0, math.rad(spinSpeed), 0)
    end
    if spasmEnabled and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(math.random(-50,50), math.random(-50,50), math.random(-50,50))
        LocalPlayer.Character.HumanoidRootPart.RotVelocity = Vector3.new(math.random(-20,20), math.random(-20,20), math.random(-20,20))
    end
    if shakeEnabled then
        local intensity = 0.5
        Camera.CFrame = Camera.CFrame * CFrame.new(math.random() * intensity - (intensity / 2), math.random() * intensity - (intensity / 2), 0)
    end
    if rainbowEnabled and LocalPlayer.Character then
        local color = Color3.fromHSV(tick() % 5 / 5, 1, 1)
        for _, v in pairs(LocalPlayer.Character:GetChildren()) do
            if v:IsA("BasePart") then v.Color = color end
        end
    end
    if drunkEnabled and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
        LocalPlayer.Character.Humanoid.WalkSpeed = math.random(5, 25)
        Camera.CFrame = Camera.CFrame * CFrame.Angles(0, 0, math.rad(math.sin(tick() * 2) * 5))
    end
    if antiSitEnabled and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
        if LocalPlayer.Character.Humanoid.Sit then
            LocalPlayer.Character.Humanoid.Sit = false
            LocalPlayer.Character.Humanoid:Jump()
        end
    end
    if fakeLagEnabled and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        if math.random(1, 10) > 5 then
            LocalPlayer.Character.HumanoidRootPart.Anchored = true
        else
            LocalPlayer.Character.HumanoidRootPart.Anchored = false
        end
    end
    if confusionEnabled then
        UserInputService.MouseIconEnabled = false
        local randomKey = Enum.KeyCode.Space
        if math.random() > 0.95 and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
             LocalPlayer.Character.Humanoid.Jump = true
        end
    else
        UserInputService.MouseIconEnabled = true
    end
    if ghostEnabled and LocalPlayer.Character then
        for _, v in pairs(LocalPlayer.Character:GetChildren()) do
            if v:IsA("BasePart") then v.Transparency = 1 end
            if v:IsA("Decal") then v.Transparency = 1 end
        end
    end
end)

task.spawn(function()
    while true do
        task.wait(0.3)
        if blackoutEnabled and (not LocalPlayer.PlayerGui:FindFirstChild("BlackoutGui")) then
            local gui = Instance.new("ScreenGui")
            gui.Name = "BlackoutGui"
            gui.ResetOnSpawn = false
            gui.IgnoreGuiInset = true
            gui.Parent = LocalPlayer.PlayerGui
            local frame = Instance.new("Frame")
            frame.Size = UDim2.new(1, 0, 1, 0)
            frame.BackgroundColor3 = Color3.new(0, 0, 0)
            frame.ZIndex = 999
            frame.Parent = gui
        end
    end
end)

local function executeCommand(cmd)
    if not isControlled then return end
    Notify("Cmd", cmd, 2)
    local char = LocalPlayer.Character
    local hum = char and char:FindFirstChild("Humanoid")
    local hrp = char and char:FindFirstChild("HumanoidRootPart")

    if cmd == "jump" then
        if hum then hum:ChangeState(Enum.HumanoidStateType.Jumping) end
    elseif cmd == "bring" then
        isBusy = true
        if char and controllerPlayer and controllerPlayer.Character then
            hrp.CFrame = controllerPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,0,2.5)
        end
        task.wait(0.5)
        isBusy = false
    elseif cmd == "spin" then
        spinEnabled = not spinEnabled
    elseif cmd == "kill" then
        isBusy = true
        char:BreakJoints()
        task.wait(1)
        isBusy = false
    elseif cmd == "void" then
        isBusy = true
        hrp.CFrame = CFrame.new(0, -500, 0)
        task.wait(1)
        isBusy = false
    elseif cmd == "jumpscare" then
        triggerJumpscare()
    elseif cmd == "fling_self" then
        isBusy = true
        hiddenfling = true
        task.wait(2)
        hiddenfling = false
        isBusy = false
    elseif cmd == "fling_nearest" then
        isBusy = true
        local nearest = getNearestPlayerTo(controllerPlayer)
        if nearest then
            flingTargetPlayer = nearest
            hiddenfling = true
            ToggleButton.Text = "ON"
            ToggleButton.BackgroundColor3 = Color3.fromRGB(0, 150, 0)
            Notify("Flinging", "Target: " .. nearest.Name, 3)
            task.wait(3)
            hiddenfling = false
            flingTargetPlayer = nil
            ToggleButton.Text = "OFF"
            ToggleButton.BackgroundColor3 = Color3.fromRGB(150, 0, 0)
        end
        isBusy = false
    elseif cmd == "stand" then
        isBusy = false
        orbitEnabled = false
        dogMode = false
    elseif cmd == "free" then
        isControlled = false
        controllerPlayer = nil
        orbitEnabled = false
        spasmEnabled = false
        shakeEnabled = false
        dogMode = false
        hiddenfling = false
        rainbowEnabled = false
        drunkEnabled = false
        antiSitEnabled = false
        fakeLagEnabled = false
        confusionEnabled = false
        ghostEnabled = false
        flingTargetPlayer = nil
        ToggleButton.Text = "OFF"
        ToggleButton.BackgroundColor3 = Color3.fromRGB(150, 0, 0)
        if signalConnection then signalConnection:Disconnect() signalConnection = nil end
        if standPlatform then standPlatform:Destroy() standPlatform = nil end
        updateStatus("STATUS: FREE", Color3.fromRGB(100, 255, 100))
        Notify("Freed", "You are no longer controlled.", 5)
    elseif cmd == "blackout" then
        blackoutEnabled = not blackoutEnabled
        if not blackoutEnabled and LocalPlayer.PlayerGui:FindFirstChild("BlackoutGui") then
            LocalPlayer.PlayerGui.BlackoutGui:Destroy()
        end
    elseif cmd == "freeze" then
        if hrp then hrp.Anchored = true end
    elseif cmd == "unfreeze" then
        if hrp then hrp.Anchored = false end
    elseif cmd == "sit" then
        if hum then hum.Sit = true end
    elseif cmd == "trip" then
        if hum then hum:ChangeState(Enum.HumanoidStateType.FallingDown) end
    elseif cmd == "orbit" then
        orbitEnabled = true
        isBusy = false
    elseif cmd == "stop_orbit" then
        orbitEnabled = false
    elseif cmd == "headless" then
        if char and char:FindFirstChild("Head") then char.Head.Transparency = 1 if char.Head:FindFirstChild("face") then char.Head.face.Transparency = 1 end end
    elseif cmd == "noarms" then
        if char then for _, v in pairs(char:GetChildren()) do if v.Name:find("Arm") or v.Name:find("Hand") then if v:IsA("BasePart") then v.Transparency = 1 end end end end
    elseif cmd == "naked" then
        if char then for _, v in pairs(char:GetChildren()) do if v:IsA("Clothing") or v:IsA("ShirtGraphic") or v:IsA("Accessory") then v:Destroy() end end end
    elseif cmd == "explode" then
        if hrp then local ex = Instance.new("Explosion") ex.Position = hrp.Position ex.Parent = workspace char:BreakJoints() end
    elseif cmd == "fakekick" then
        LocalPlayer:Kick("You have been kicked by the Controller.")
    elseif cmd == "blur" then
        local bl = Instance.new("BlurEffect") bl.Name = "ControlledBlur" bl.Size = 24 bl.Parent = Lighting
    elseif cmd == "unblur" then
        for _, v in pairs(Lighting:GetChildren()) do if v.Name == "ControlledBlur" then v:Destroy() end end
    elseif cmd == "screenshake" then
        shakeEnabled = not shakeEnabled
    elseif cmd == "force1st" then
        LocalPlayer.CameraMode = Enum.CameraMode.LockFirstPerson
    elseif cmd == "fixcam" then
        LocalPlayer.CameraMode = Enum.CameraMode.Classic
        shakeEnabled = false
        Camera.FieldOfView = 70
    elseif cmd == "slow" then
        if hum then hum.WalkSpeed = 4 end
    elseif cmd == "fast" then
        if hum then hum.WalkSpeed = 100 end
    elseif cmd == "lowgrav" then
        workspace.Gravity = 10
    elseif cmd == "normgrav" then
        workspace.Gravity = 196.2
    elseif cmd == "spasm" then
        spasmEnabled = true
    elseif cmd == "unglitch" then
        spasmEnabled = false
        rainbowEnabled = false
        drunkEnabled = false
        antiSitEnabled = false
        fakeLagEnabled = false
        confusionEnabled = false
        ghostEnabled = false
        if hrp then hrp.Anchored = false end
        if hum then hum.Sit = false hum.WalkSpeed = 16 end
        workspace.Gravity = 196.2
        dogMode = false
        Camera.FieldOfView = 70
    elseif cmd == "float" then
        if hrp then local b = Instance.new("BodyVelocity") b.Name = "FloatVel" b.Velocity = Vector3.new(0,10,0) b.MaxForce = Vector3.new(0,math.huge,0) b.Parent = hrp task.delay(3, function() b:Destroy() end) end
    elseif cmd == "sink" then
         if hrp then hrp.CFrame = hrp.CFrame * CFrame.new(0, -10, 0) end
    elseif cmd == "dog" then
        dogMode = true
        if hum then hum.PlatformStand = true end
    elseif cmd == "undog" then
        dogMode = false
        if hum then hum.PlatformStand = false end
    elseif cmd == "worship" then
        if hum then hum.Sit = true end
        if hrp and controllerPlayer and controllerPlayer.Character then
            hrp.CFrame = CFrame.new(hrp.Position, controllerPlayer.Character.HumanoidRootPart.Position)
        end
    elseif cmd == "rocket" then
        if hrp then 
            local bv = Instance.new("BodyVelocity")
            bv.Velocity = Vector3.new(0, 500, 0)
            bv.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
            bv.Parent = hrp
            task.wait(1)
            bv:Destroy()
        end
    elseif cmd == "blockhead" then
        if char and char:FindFirstChild("Head") and char.Head:FindFirstChild("Mesh") then
            char.Head.Mesh:Destroy()
        end
    elseif cmd == "creepy" then
        if hum then hum.HipHeight = -1.5 end
    elseif cmd == "rainbow" then
        rainbowEnabled = true
    elseif cmd == "unrainbow" then
        rainbowEnabled = false
    elseif cmd == "ghost" then
        ghostEnabled = true
    elseif cmd == "unghost" then
        ghostEnabled = false
    elseif cmd == "highfov" then
        Camera.FieldOfView = 120
    elseif cmd == "lowfov" then
        Camera.FieldOfView = 10
    elseif cmd == "normalfov" then
        Camera.FieldOfView = 70
    elseif cmd == "drunk" then
        drunkEnabled = true
    elseif cmd == "sober" then
        drunkEnabled = false
    elseif cmd == "fakelag" then
        fakeLagEnabled = not fakeLagEnabled
    elseif cmd == "antisit" then
        antiSitEnabled = not antiSitEnabled
    elseif cmd == "mock" then
        pcall(function() Chat:Chat(hrp, "I AM A NOOB AND I LOVE IT!", Enum.ChatColor.Red) end)
    elseif cmd == "earthquake" then
        shakeEnabled = true
        Camera.CFrame = Camera.CFrame * CFrame.Angles(0,0, math.rad(25))
    elseif cmd == "confuse" then
        confusionEnabled = true
    elseif cmd == "clarity" then
        confusionEnabled = false
    end
end

local function initSignalListener(master)
    if signalConnection then signalConnection:Disconnect() end
    local hum = master.Character and master.Character:FindFirstChild("Humanoid")
    if hum then
        local animator = hum:FindFirstChild("Animator") or hum:WaitForChild("Animator")
        signalConnection = animator.AnimationPlayed:Connect(function(track)
            local id = track.Animation.AnimationId
            local cleanID = string.match(id, "%d+")
            if SIGNALS[cleanID] then
                executeCommand(SIGNALS[cleanID])
            end
        end)
    end
    master.CharacterAdded:Connect(function(newChar)
        local h = newChar:WaitForChild("Humanoid")
        local a = h:WaitForChild("Animator")
        if signalConnection then signalConnection:Disconnect() end
        signalConnection = a.AnimationPlayed:Connect(function(track)
            local id = track.Animation.AnimationId
            local cleanID = string.match(id, "%d+")
            if SIGNALS[cleanID] then
                executeCommand(SIGNALS[cleanID])
            end
        end)
    end)
end

local function broadcastSignal(id)
    if LocalPlayer.Character then
        local hum = LocalPlayer.Character:FindFirstChild("Humanoid")
        local animator = hum and hum:FindFirstChild("Animator")
        if animator then
            local anim = Instance.new("Animation")
            anim.AnimationId = "rbxassetid://" .. id
            local track = animator:LoadAnimation(anim)
            track.Priority = Enum.AnimationPriority.Action
            track:Play()
            task.wait(0.1)
            track:Stop()
        end
    end
end

local MainTab = Window:CreateTab("Be Controlled", "cat")
local connectInputName = ""

MainTab:CreateLabel("Type a player's name to become their Stand/Controlled.")
MainTab:CreateInput({
    Name = "Controller's Name",
    PlaceholderText = "Type Player Name to OBEY",
    RemoveTextAfterFocusLost = false,
    Callback = function(Text)
        connectInputName = Text
    end,
})
MainTab:CreateButton({
    Name = "Submit & Obey",
    Callback = function()
        local t = findPlayerPartial(connectInputName)
        if t and t ~= LocalPlayer then
            controllerPlayer = t
            isControlled = true
            initSignalListener(t)
            updateStatus("CONTROLLED (Controlled by " .. t.Name .. ")", Color3.fromRGB(255, 50, 50))
            Notify("Submitted", "You are now controlled by " .. t.Name .. ".", 5)
        else
            Notify("Error", "Player invalid or not found", 3)
        end
    end,
})

